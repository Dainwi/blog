---
// component import
import MainHead from "./MainHead.astro";
import Footer from "../components/Footer.astro";
import Nav from "../components/Nav.astro";

// style import
import "../styles/global.css";

const {
  title = "Dainwi's Blog",
  description = "I am Dainwi Choudhary, exploring philosophy and psychology to offer practical insights for personal growth, resilience, and balanced living.",
  image,
  frontmatter,
  robots,
} = Astro.props;
---

<html lang="en">
  <MainHead {title} {description} {image} {frontmatter} {robots} />
  <body>
    <Nav />
    <main>
      <slot> </slot>
    </main>
    <Footer />
  </body>
</html>
